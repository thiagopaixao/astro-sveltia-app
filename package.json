{
  "name": "app-git-electron",
  "version": "1.0.0",
  "description": "Ambiente de documentação centralizado com integração Git",
  "main": "main.js",
  "homepage": "https://github.com/documental/app-git-electron",
"scripts": {
    "start": "electron main.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "build:css": "npx tailwindcss -i renderer/assets/css/main.css -o renderer/assets/css/compiled.css --postcss",
    "build": "npm run build:css && electron-builder",
    "build:linux": "npm run prebuild:linux && electron-builder --linux",
    "build:linux:deb": "npm run prebuild:linux && electron-builder --linux deb",
    "build:linux:dir": "npm run prebuild:linux && electron-builder --linux dir",
    "build:linux:snap": "npm run prebuild:linux && electron-builder --linux snap",
    "build:linux:appimage": "npm run prebuild:linux && electron-builder --linux AppImage",
    "build:win": "npm run prebuild:windows && electron-builder --win portable",
    "build:macos": "npm run prebuild:macos && electron-builder --mac",
    "build:all": "npm run prebuild:all && electron-builder -mwl",
    "dist": "npm run build && node scripts/post-build.js",
    "clean": "rimraf dist",
    "prebuild:windows": "node scripts/download-node-binaries.js win32 x64",
    "prebuild:linux": "node scripts/download-node-binaries.js linux x64",
    "prebuild:macos": "node scripts/download-node-binaries.js darwin x64",
    "prebuild:all": "npm run prebuild:windows && npm run prebuild:linux && npm run prebuild:macos",
    "validate:all": "npm run validate:extensions && npm run validate:es2022 && npm run validate:jsdoc && npm run validate:hybrid",
    "validate:extensions": "node project/scripts/validate-extensions.mjs",
    "validate:es2022": "node project/scripts/validate-es2022.mjs",
    "validate:jsdoc": "node project/scripts/validate-jsdoc.mjs",
    "validate:hybrid": "node project/scripts/validate-hybrid.mjs"
  },
  "keywords": [
    "documentation",
    "git",
    "github",
    "development"
  ],
  "author": {
    "name": "Thiago Paixao",
    "email": "tap@thiagopaixao.com"
  },
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@vitest/ui": "^4.0.6",
    "asar": "^3.2.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "electron": "^38.2.0",
    "electron-builder": "^25.1.8",
    "jsdom": "^27.1.0",
    "postcss": "^8.4.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "alpinejs": "^3.13.0",
    "call-bind": "^1.0.8",
    "call-bind-apply-helpers": "^1.0.2",
    "call-bound": "^1.0.4",
    "dunder-proto": "^1.0.1",
    "es-errors": "^1.3.0",
    "execa": "^9.0.0",
    "fs-extra": "^11.0.0",
    "get-intrinsic": "^1.3.0",
    "get-proto": "^1.0.1",
    "gopd": "^1.0.1",
    "has-proto": "^1.0.3",
    "has-symbols": "^1.0.3",
    "isomorphic-git": "^1.34.0",
    "keytar": "^7.9.0",
    "sqlite3": "^5.1.7",
    "tar": "^7.5.2",
    "yauzl": "^3.2.0"
  },
  "build": {
    "appId": "com.documental.app",
    "productName": "Documental",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "renderer/**/*",
      "src/**/*",
      "package.json",
      "node_modules/**/*",
      "!node_modules/.cache/**",
      "!node_modules/.bin/**",
      "!node_modules/**/test/**",
      "!node_modules/**/tests/**",
      "!node_modules/**/example/**",
      "!node_modules/**/examples/**",
      "!node_modules/**/*.md",
      "!node_modules/**/*.txt",
      "!node_modules/**/CHANGELOG*",
      "!node_modules/**/LICENSE*",
      "!node_modules/**/README*"
    ],
    "asarUnpack": [
      "node_modules/call-bind-apply-helpers/**/*",
      "node_modules/es-errors/**/*",
      "node_modules/gopd/**/*",
      "node_modules/has-proto/**/*",
      "node_modules/has-symbols/**/*",
      "node_modules/call-bind/**/*",
      "node_modules/get-intrinsic/**/*",
      "node_modules/get-proto/**/*",
      "node_modules/dunder-proto/**/*",
      "node_modules/call-bound/**/*",
      "node_modules/sqlite3/**/*",
      "node_modules/keytar/**/*"
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "snap",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "assets/icon.png",
      "category": "Development",
      "desktop": {
        "Name": "Documental",
        "Comment": "Ambiente de documentação centralizado com integração Git",
        "Keywords": "documentation;git;github;development;"
      },
      "maintainer": "Thiago Paixão <tap@thiagopaixao.com>"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.png",
      "sign": false,
      "signDlls": false
    },
    "snap": {
      "plugs": [
        "default",
        "desktop",
        "desktop-legacy",
        "home",
        "x11",
        "unity7",
        "browser-support",
        "network",
        "gsettings",
        "audio-playback",
        "pulseaudio",
        "opengl"
      ]
    }
  }
}
